{
  "name": "apex-trigger-framework-scaffold",
  "displayName": "Create Apex Trigger Framework Scaffold",
  "publisher": "shisoderohan49",
  "description": "Creates Skeletion/Scaffold for your Apex Trigger Framework quickly (based on Kevin O'Hara's framework)",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "apex-trigger-framework-scaffold.createApexTriggerFrameworkSkeletion",
        "title": "Create Apex Trigger Framework Skeleton"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shisoderohan49/Apex-Trigger-Framework-Scaffold"
  },
  "homepage": "https://github.com/shisoderohan49/Apex-Trigger-Framework-Scaffold/blob/master/README.md",
  "changelog": "https://github.com/shisoderohan49/Apex-Trigger-Framework-Scaffold/blob/master/CHANGELOG.md"
}